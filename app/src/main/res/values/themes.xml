<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Voting_final_year_project" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryDark">@color/purple_700</item>
        <item name="colorAccent">@color/teal_200</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="prefix" parent="Base.TextAppearance.AppCompat.Body2">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="Theme.Voting_final_year_project.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Voting_final_year_project.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Voting_final_year_project.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>